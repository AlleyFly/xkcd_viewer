<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="750.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainWindowController">
   <children>
      <HBox alignment="CENTER" spacing="100.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <TableView fx:id="tabFavorites" prefHeight="70.0" prefWidth="200.0">
              <columns>
                <TableColumn fx:id="colNumber" prefWidth="75.0" text="Number" />
                <TableColumn fx:id="colTitle" prefWidth="115.0" text="Title" />
              </columns>
            </TableView>
            <Button fx:id="favButton" mnemonicParsing="false" onAction="#favorite" text="Favorite" />
            <VBox alignment="CENTER" layoutX="382.0" layoutY="391.0">
               <children>
                  <HBox>
                     <children>
                        <Button fx:id="prevButton" mnemonicParsing="false" onAction="#loadPrev" text="&lt;" />
                        <TextField fx:id="numberField" layoutX="382.0" layoutY="391.0" promptText="Number" />
                        <Button fx:id="nextButton" mnemonicParsing="false" onAction="#loadNext" text="&gt;" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER">
                     <children>
                        <Button fx:id="specifiedButton" alignment="CENTER" layoutX="417.0" layoutY="432.0" mnemonicParsing="false" onAction="#loadNumber" text="load number" />
                        <Button fx:id="recentButton" mnemonicParsing="false" onAction="#loadRecent" text="newest" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
      <ScrollPane fx:id="scrollPane" layoutX="50.0" layoutY="30.0" prefHeight="350.0" AnchorPane.bottomAnchor="150.0" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="30.0">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <content>
            <StackPane fx:id="stackPane">
               <children>
                  <ImageView fx:id="imageView" pickOnBounds="true" preserveRatio="true" />
               </children>
            </StackPane>
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
